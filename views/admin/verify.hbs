<div class="row centered mt-4">
    <div class="col-md-7 col-md-offset-1 mx-auto">
        <center>
        <table class="table table-hover table-striped table-dark table-hover">
            <h1>Pending users</h1>
             
            <thead>
                <tr>
                    {{!-- <th scope="col" class="pl-5 pr-5">License Number</th> --}}
                    <th scope="col"class="pl-5 pr-5">Number</th>
                     <th scope="col"class="pl-5 pr-5">Name</th>
                     <th scope="col"class="pl-5 pr-5">License</th>
                    <th scope="col" class="pl-5 pr-3">Accept</th>
                    <th scope="col" class="pl-5 pr-3">Reject</th>
                     


                </tr>
            </thead>
            <tbody>
                {{#each data}}
                
                <tr>
                    
                     <th scope="row"class="pl-5 pr-5">{{this.mobile}} </th>
                     <th scope="row"class="pl-5 pr-5">{{this.name}} </th>
                     <th scope="row"class="pl-5 pr-5">{{this.license}} </th>
                     
                    <th scope="row"class="pl-5 pr-5">
                        <form method="post" action="/acceptverification/{{this._id}}">
                
                    <button type="submit" class="btn btn-success">-> </button>
                </form>
                        
                    </th>
                    <th scope="row"class="pl-5 pr-5">
                        <form method="post" action="/rejectverification/{{this._id}}">
                
                    <button type="submit" class="btn btn-danger">X </button>
                </form>
                        
                    </th>
                    
                    {{!-- --}}
                    <!-- Modal -->
<!-- Modal -->
  
  </div>
                   

                </tr>
                {{/each}}
            </tbody>
        </table>
    </center>
    </div>
</div>